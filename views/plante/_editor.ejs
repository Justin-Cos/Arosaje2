<div class="form-group row">
  <label class="col-form-label col-sm-2" for="plantName">plantName</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" name="plantName" value="<%= model.plantName %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="famille">famille</label>
  <div class="col-sm-8">
    <input class="form-control" name="famille" value="<%= model.famille %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="location">location</label>
  <div class="col-sm-10">
    <textarea class="form-control" cols="20" name="location" maxlength="32000" rows="7"><%= model.location %></textarea>
  </div>
</div>
